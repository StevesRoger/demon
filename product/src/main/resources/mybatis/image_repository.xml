<!DOCTYPE mapper PUBLIC "-//mapper.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jarvis.ws.medicine.repository.ImageRepository">

    <insert id="addImage" parameterType="Map" useGeneratedKeys="true" keyProperty="id" keyColumn="id">
        INSERT INTO ph_image (mine_type, "name", path, bytes, url, created_by)
        VALUES (#{mineType} ,#{name}, #{path}, #{bytes}, #{url}, #{username})
    </insert>

    <select id="getById" resultType="ImageEntity">
        SELECT * FROM ph_image WHERE id = #{id}
    </select>

</mapper>